		"glob": "=10.3.7",
		"in-publish": "^2.0.1",
		"is-core-module": "^2.16.1",
		"jackspeak": "=2.1.1",
		"npmignore": "^0.3.5",
		"nyc": "^10.3.2",
		"object-inspect": "^1.13.4",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.9.0"
	},
	"dependencies": {
		"call-bind": "^1.0.8",
		"call-bound": "^1.0.4",
		"define-properties": "^1.2.1",
		"es-abstract": "^1.24.0",
		"es-errors": "^1.3.0",
		"get-intrinsic": "^1.3.0",
		"math-intrinsics": "^1.1.0",
		"safe-array-concat": "^1.1.3",
		"safe-regex-test": "^1.1.0",
		"typed-array-buffer": "^1.0.3",
		"typed-array-byte-offset": "^1.0.4",
		"which-typed-array": "^1.1.19"
	},
	"engines": {
		"node": ">= 0.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	},
	"testling": {
		"files": "test/shimmed.js"
	}
